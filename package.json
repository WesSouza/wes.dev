{
  "name": "wes.dev",
  "version": "2020.0.0",
  "description": "https://wes.dev/",
  "scripts": {
    "build": "NODE_ENV=production parcel build src/index.html --experimental-scope-hoisting",
    "dev": "parcel src/index.html",
    "now-build": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesSouza/wes.dev.git"
  },
  "keywords": [
    "personal website",
    "Wes Souza",
    "Wesley de Souza"
  ],
  "author": "Wes Souza <hey@wes.dev>",
  "license": "MIT",
  "homepage": "https://wes.dev/",
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "import-sort-style-wes": "^1.0.1",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "prettier-plugin-import-sort": "^0.0.3"
  },
  "browserslist": [
    "last 2 and_chr versions",
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 2 ios_saf versions",
    "last 2 safari versions"
  ],
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "wes"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "wes"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
