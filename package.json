    "astro": "^4.5.2",
    "rollup": "^4.13.0",
    "solid-js": "^1.8.15"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.7",
    "@types/node": "20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "import-sort-style-wes": "^3.0.2",
    "prettier": "3.2.5",
    "prettier-plugin-import-sort": "^0.0.7",
    "ts-node": "^10.9.2",
    "typescript": "5.4.2"
