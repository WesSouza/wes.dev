  },
  "devDependencies": {
    "@astrojs/check": "^0.7.0",
    "@eslint/js": "^9.5.0",
    "@types/node": "20.14.5",
    "dotenv": "^16.4.5",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.6.0",
    "import-sort-style-wes": "^3.0.2",
