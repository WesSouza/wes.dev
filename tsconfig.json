{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "",
    "importHelpers": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "lib": ["DOM", "ESNext"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {},
    "rootDir": ".",
    "sourceMap": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "verbatimModuleSyntax": true
  },
  "exclude": [".vercel", "node_modules", "public"]
}
